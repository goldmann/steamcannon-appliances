name: front-end
summary: front-end appliance with httpd, mod_cluster
version: 1
release: 0
appliances:
  - steamcannon-base
packages:
  includes:
    - mod_cluster
    - httpd
post:
  base:
    - "/sbin/chkconfig --level 234 httpd on"
    - "echo 'log_level: trace' > /etc/sysconfig/steamcannon-agent"

